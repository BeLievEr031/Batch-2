{
    "manifest_version": 3,
    "name": "Color Picker",
    "version": "1.0",
    "description": "A Chrome extension to pick colors from a webpage.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "./popup.html",
        "default_icon": "./icon.png"
    },
    "icons": {
        "16": "icon.png"
    }
}